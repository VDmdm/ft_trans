
<table>
	<tr>
		<th>id</th>
		<th>nickname</th>
		<th>email</th>
		<th>action</th>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.id %></td>
			<td><%= user.nickname %></td>
			<td><%= user.email %></td>
			<% if current_user.guild_owner && !user.guild %>
				<td><%= link_to "add to guild", add_member_path(current_user.guild, user_id: user), method: :post, remote: true %></td></tr>
			<% end %>
	<% end %>
</table>

<style>
	td, th {
		color: white;
	}
</style>